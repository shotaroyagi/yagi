<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.javaclub.domain">
 <class name="Shop" table="shop">
 <id name="id" column="id">
 <generator class="native" />
 </id>
<many-to-one name="booth" column="booth_name" />
 <property name="shopName" column="shop_name" type="string" />
 <property name="group" column="group" type="string" />
 </class>
</hibernate-mapping>